subroutine tglf

  !Subroutine to write the inputs into TGLF Quasilinear code
  ! B Patel 24/05/2018
  
  use param
  implicit none

  integer :: ndsk, ios

  ndsk=67
  open(unit=ndsk, file=runname(1:lrunname)//'_profile.tglf', &
       status='unknown', iostat=ios)
  if(ios.ne.0) then
     write(6,*) 'problem creating/opening ',runname(1:lrunname) &
          //'_profile.tglf'
     stop
  endif



end subroutine tglf
