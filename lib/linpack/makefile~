SUBS =  dqrdc.o dqrsl.o dtrco.o dtrsl.o\
	dchdc.o dsvdc.o


SRCS =  dqrdc.f dqrsl.f dtrco.f dtrsl.f\
	dchdc.f dsvdc.f

EXE = liblinpack.a

LIBRARIES = \
 -L/hwdisks/home/bbp501/libscene/ \
 -lBLAS


COMPILE = ifort -c
LOAD =  ar rc 

all:	$(EXE)


$(EXE): 		$(SUBS)
	$(LOAD) $(EXE)  $(SUBS) 
	cp $(EXE) ../

.f.o:
	$(COMPILE) $(LIBRARIES) $<

clean:
	rm -f *.o
